package utils;

import java.util.Vector;

import dataInVector.DataDepQuoti;

/**
 * A class simulated as Database, therefore it only contains static methods and varaibles
 * @author MiHu
 */

public class DatabaseSim<T> {

	public static Vector<T> m_vt_depQuoti = new Vector<T>();
	
	public DatabaseSim()
	{}
	
	public static void addToDB(T obj)
	{
		m_vt_depQuoti.addElement(obj);
	}
}
