package gui;

import java.awt.Dimension;

import javax.swing.ImageIcon;
import javax.swing.JPanel;
import javax.swing.JTabbedPane;
import javax.swing.SwingConstants;

@SuppressWarnings("serial")
public class TabPane extends JTabbedPane {
	
	public TabPane()
	{
		super();
		
		ImageIcon icon_depense = new ImageIcon(getClass().getResource("/depense.png"));
		JPanel pn_depense = new JPanel();
		//pn_depense.setPreferredSize(new Dimension(500, 400));
		addTab("Dépense quotidienne", icon_depense, pn_depense, "Dépense");
	}

}
