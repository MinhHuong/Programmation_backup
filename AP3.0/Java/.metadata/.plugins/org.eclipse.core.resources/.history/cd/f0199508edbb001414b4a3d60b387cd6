package utils;

import java.util.Vector;

import dataInVector.DataDepQuoti;

/**
 * A class simulated as Database, therefore it only contains static methods and varaibles
 * @author MiHu
 */

public class DatabaseSim {

	public static Vector<DataDepQuoti> m_vt_depQuoti = new Vector<DataDepQuoti>();
	
	public DatabaseSim()
	{}
	
	public addToDB(DataDepQuoti obj)
	{
		m_vt_depQuoti.addElement(obj);
	}
}
