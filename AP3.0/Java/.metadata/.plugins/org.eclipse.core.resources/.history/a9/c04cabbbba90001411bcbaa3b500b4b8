package gui.depense;

public class MyData {

	private String m_nom;
	private int m_somme;
	private boolean m_paye;
	
	public MyData(String nom, int somme, boolean paye)
	{
		m_nom = nom;
		m_somme = somme;
		m_paye = paye;
	}
	
	public String getIntitule()
	{
		return m_nom;
	}
	
	public int getSomme()
	{
		return m_somme;
	}
}
